<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="accessLogServiceDao" class="org.mybatis.spring.mapper.MapperFactoryBean">
		<property name="mapperInterface" value="org.izhong.expert.dao.AccessLogServiceDao"></property>
		<property name="sqlSessionFactory" ref="sqlSessionFactory"></property>
	</bean>
	<bean name="accessLogService" class="org.izhong.expert.service.AccessLogServiceImpl"> 
		<property name="accessLogServiceDao" ref="accessLogServiceDao"/>
	</bean>
</beans>
