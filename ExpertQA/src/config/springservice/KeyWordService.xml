<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="keyWordDao" class="org.mybatis.spring.mapper.MapperFactoryBean">
		<property name="mapperInterface" value="org.izhong.expert.dao.KeyWordDao"></property>
		<property name="sqlSessionFactory" ref="sqlSessionFactory"></property>
	</bean>
	<bean name="keyWordService" class="org.izhong.expert.service.KeyWordServiceImpl">
		<property name="keyWordDao" ref="keyWordDao"/>
	</bean>
</beans>
